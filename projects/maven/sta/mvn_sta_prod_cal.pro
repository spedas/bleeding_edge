			eff_stop[i]  = tmp[i,mind,7]/((tmp[i,mind,10]-ab_bgnd*(1.+1./ab_bgnd^.5))>1.)
			eff[i] = eff_start[i] * eff_stop[i]

			fqual[i] = tmp[i,mind,7]/(tmp[i,mind,4]-bgnd*(1.+1./bgnd^.5))
			fqual2[i]= (1. + (1.-eff_start[i])*(1.-eff_stop[i])) * eff[i]
			btrst[i] = bgnd
